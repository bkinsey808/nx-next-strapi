{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "vscode-graphql.rejectUnauthorized": false,
  "vscode-graphql.debug": true,
  "vscode-graphql.trace.server": "on",
  "vscode-graphql.showExecCodelens": false,
  "cSpell.words": ["codegen", "dedup", "devkit", "nrwl", "strapi", "urql"],
  "cSpell.ignorePaths": ["apps/client/graphql.tsx", "settings.json"]
}
