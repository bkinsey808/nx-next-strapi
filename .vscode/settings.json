{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "vscode-graphql.rejectUnauthorized": false,
  "vscode-graphql.debug": true,
  "vscode-graphql.trace.server": "on",
  "vscode-graphql.showExecCodelens": false,
  "cSpell.words": [
    "Buildable",
    "codegen",
    "dedup",
    "devkit",
    "firstname",
    "hookform",
    "kumar",
    "lastname",
    "nrwl",
    "SSG'd",
    "strapi",
    "svgr",
    "tailwindcss",
    "Unmemoized",
    "urql"
  ],
  "cSpell.workspaceRootPath": "./apps",
  "cSpell.ignorePaths": [
    "/home/bkinsey/**",
    "../../**",
    "!./apps/client/**",
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".vscode-server/**",
    ".vscode-insiders",
    "package.json",
    "*.initialState.json",
    "*.testData.json",
    "**/*.d.ts",
    ".vscode-server",
    "apps/client/graphql.tsx",
    "./apps/client/graphql.tsx",
    "settings.json"
  ]
}
